<!doctype html>
<html lang="en" ng-app="kaufland">
<head>
  <meta charset="UTF-8">
  <title>Document</title>
  <link rel="stylesheet" href="/css/grid12.css">
  <link rel="stylesheet" href="/css/style.css">
  <link rel="stylesheet" href="/libs/angular-ui-bootstrap/dist/ui-bootstrap-csp.css">
  <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.6/css/bootstrap.min.css">
</head>
<body ng-controller="kauflandCtrl as kauf">
  <div class="wrapper container kauf-container">
    <header class="row">
      <nav class="kauf-nav col-md-6 col-sm-6">
        <ul>
          <li>
            <a href="javascript:void(0)">item 1</a>
            <ul class="sub-nav">
              <li><a href="javascript:void(0)">sub item 1</a></li>
              <li>
                <a href="javascript:void(0)">sub item 2</a>
                <ul class="sub-sub-nav">
                  <li><a href="javascript:void(0)">sub sub item 1</a></li>
                  <li><a href="javascript:void(0)">sub sub item 2</a></li>
                  <li><a href="javascript:void(0)">sub sub item 3</a></li>
                </ul>
              </li>
              <li><a href="javascript:void(0)">sub item 3</a></li>
            </ul>
          </li>
          <li>
            <a href="javascript:void(0)">item 2</a>
          </li>
          <li>
            <a href="javascript:void(0)">item 3</a>
          </li>
        </ul>
      </nav>
      <h3 class="col-md-6 col-sm-6">header</h3>
    </header>
    <main class="row">
        <div class="content col-md-10 col-sm-10">
          <div>
            <label for="select">Choose article ID:</label>
            <select ng-options="item.id as item.id for item in kauf.selectData"
                    ng-model="kauf.selected" id="select"></select>
          </div>
          <h3>{{ kauf.selectDataItem.title }}</h3>
          <p>{{ kauf.selectDataItem.body }}</p>


          <button type="button" ng-click="kauf.open()">Open Modal</button>
        </div>
        <div class="context col-md-2 col-sm-2">
          context
        </div>
    </main>
    <footer class="row">
        <h3 class="col-md-12 col-sm-12">footer</h3>
    </footer>
  </div>
  <script src="/libs/angular/angular.min.js"></script>
  <script src="/libs/angular-ui-bootstrap/dist/ui-bootstrap.js"></script>
  <script src="/libs/angular-ui-bootstrap/dist/ui-bootstrap-tpls.js"></script>
  <script src="/js/app.js"></script>
  <script src="/js/services/posts.service.js"></script>
</body>
</html>
